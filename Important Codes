#Extracting Zip file
import zipfile
path_to_zip_file='/content/Facebook-20200618T044353Z-001.zip'
directory_to_extract_to='/content/facebook'
with zipfile.ZipFile(path_to_zip_file, 'r') as zip_ref:
    zip_ref.extractall(directory_to_extract_to)
